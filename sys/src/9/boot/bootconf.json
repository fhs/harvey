[
	{
		"Name": "bootcpu",
		"Include": [
			"/$ARCH/cflags.json"
		],
		"Libs": [
			"libboot.a",
			"/$ARCH/lib/libip.a",
			"/$ARCH/lib/libauth.a",
			"/$ARCH/lib/libc.a"
		],
		"Oflags": [
			"-e_main",
			"-static"
		],
		"Post": [
			"rm -f *.o *.a",
			"go run github.com/Harvey-OS/harvey/util/src/harvey/cmd/data2c ramfs_bootcpu_ ../boot/bootcpu.elf.out ../$ARCH/cpu.c"
		],
		"Program": "bootcpu.elf.out",
		"Projects": [
			"boot.json"
		],
		"SourceFiles": [
			"printstub.c",
			"bootcpu.c"
		]
	}
]
